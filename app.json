{
  "expo": {
    "name": "Ash-Ai",
    "slug": "Ash-Ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ash-logo.png",
    "scheme": "ashai",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/ash-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.joe1724.ashai"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/ash-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ash-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d59d1caa-d04a-4386-8446-168f6f1633f4"
      }
    }
  }
}
